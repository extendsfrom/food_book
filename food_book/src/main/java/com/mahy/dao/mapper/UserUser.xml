<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mahy.dao.UserDao">
	<insert id="insertUser" parameterType="com.mahy.vo.UserUser">
		insert into user_user(name, password) values (#{name}, #{password})
	</insert>
</mapper>
